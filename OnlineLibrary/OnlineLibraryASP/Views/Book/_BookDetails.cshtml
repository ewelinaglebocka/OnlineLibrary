@using OnlineLibrary.BLL.Models
@model Book
<h4>Book</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label asp-for="Id"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.Id
        </dd>
        <dt class = "col-sm-2">
            <label asp-for="Title"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.Title
        </dd>
        <dt class = "col-sm-2">
            <label asp-for="Author"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.Author
        </dd>
        <dt class = "col-sm-2">
            <label asp-for="BookType"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.BookType
        </dd>
        <dt class = "col-sm-2">
            <label asp-for="PublicationDate"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.PublicationDate
        </dd>
    </dl>